{
  "name": "ul-imports",
  "version": "1.0.0",
  "description": "The scripts used to import data into the Unified Listing.",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test": "node node_modules/istanbul/lib/cli.js cover tests/all-tests.js",
    "full-sync": "npm run all-imports && npm run unifier && npm run all-image-imports",
    "all-imports": "npm run eastin-import && npm run sai-import && npm run gari-import",
    "eastin-import": "node src/js/eastin/launcher.js --optionsFile %ul-imports/configs/eastin-prod.json",
    "sai-import": "node src/js/sai/index.js --optionsFile %ul-imports/configs/sai-prod.json",
    "gari-import": "node src/js/gari/index.js --optionsFile %ul-imports/configs/gari-prod.json",
    "unifier": "node src/js/unifier/index.js --optionsFile %ul-imports/configs/unifier-prod.json",
    "all-image-imports": "npm run eastin-image-import && npm run sai-image-import && npm run gari-image-import",
    "eastin-image-import": "node src/js/images/eastin-image-sync.js --optionsFile %ul-imports/configs/eastin-image-sync-prod.json",
    "sai-image-import": "node src/js/images/sai-image-sync.js --optionsFile %ul-imports/configs/sai-image-sync-prod.json",
    "gari-image-import": "node src/js/images/gari-image-sync.js --optionsFile %ul-imports/configs/gari-image-sync-prod.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/the-t-in-rtf/ul-import.git"
  },
  "author": "Tony Atkins <tony@raisingthefloor.org>",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/the-t-in-rtf/ul-import/issues"
  },
  "homepage": "https://github.com/the-t-in-rtf/ul-import#readme",
  "dependencies": {
    "deep-equal": "1.0.1",
    "gpii-express": "1.0.7",
    "gpii-express-user": "1.0.0-dev.20170420T094715Z.667e69a",
    "gpii-launcher": "1.0.0-dev.20170504T113435Z.c5b7001",
    "gpii-pouchdb-lucene": "0.1.0-dev.20170523T095210Z.7508cd5",
    "gpii-sort": "1.0.0-dev.20170420T114825Z.804ac19",
    "infusion": "3.0.0-dev.20170322T234120Z.278de35",
    "md5-file": "3.1.1",
    "mkdirp": "0.5.1",
    "universal": "GPII/universal#88aec55cd2cafb2cab914978b11c5e1bc7926260"
  },
  "devDependencies": {
    "eslint": "3.19.0",
    "eslint-config-fluid": "1.1.0",
    "fluid-grunt-eslint": "18.1.2",
    "grunt": "1.0.1",
    "grunt-jsonlint": "1.1.0",
    "istanbul": "0.4.5",
    "kettle": "1.5.0",
    "node-jqunit": "1.1.4",
    "request": "2.81.0",
    "ul-api": "1.0.0-dev.20170608T121153Z.0f7693c"
  }
}
